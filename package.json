{
  "name": "griffeath-machine",
  "version": "1.0.0",
  "description": "Cyclic cellular automaton",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --mode development --hot --open",
    "build": "webpack --mode production",
    "deploy": "yarn build && gh-pages -d dist",
