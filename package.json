    "postcss-modules": "^1.4.1",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "webpack": "^4.32.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "worker-loader": "^2.0.0"
  },
  "eslintConfig": {
    "extends": "air"
